{
  "name": "@shelf/eslint-config",
  "version": "5.3.2",
  "description": "ESLint Config for Shelf Projects",
  "license": "MIT",
  "repository": "shelfio/eslint-config",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "test": "echo ok",
    "prepare": "husky",
    "lint": "eslint . --fix"
  },
  "files": [
    "common",
    "rules",
    "base.js",
    "frontend-typescript.js",
    "typescript.js"
  ],
  "main": "base.js",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "type": "module",
  "dependencies": {
    "@eslint/compat": "2.0.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@stylistic/eslint-plugin": "5.6.1",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.2.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx": "0.1.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-shelf-no-need-lodash-methods": "2.0.8",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-testing-library": "7.13.4",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.14.0",
    "globals": "16.5.0",
    "typescript-eslint": "8.47.0"
  },
  "devDependencies": {
    "eslint": "9.39.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2"
  },
  "peerDependencies": {
    "eslint": "9.x",
    "prettier": "3.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{html,md,yml}": "prettier --write",
    "*.{ts,js,json}": "eslint --fix"
  }
}
