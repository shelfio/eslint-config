{
  "name": "@shelf/eslint-config",
  "version": "5.0.2",
  "description": "ESLint Config for Shelf Projects",
  "license": "MIT",
  "repository": "shelfio/eslint-config",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "test": "echo ok",
    "prepare": "husky",
    "inspect-config": "npx @eslint/config-inspector@latest"
  },
  "files": [
    "common",
    "plugins",
    "rules",
    "base.js",
    "frontend-typescript.js",
    "typescript.js",
    "typescript-v2.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "resolutions": {
    "strip-ansi": "6.0.1"
  },
  "type": "module",
  "dependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-shelf-no-need-lodash-methods": "2.0.8",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-testing-library": "7.6.6",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.14.0",
    "globals": "16.3.0",
    "typescript-eslint": "8.40.0"
  },
  "devDependencies": {
    "@shelf/tsconfig": "0.1.0",
    "@types/eslint__js": "9.14.0",
    "eslint": "9.33.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "eslint": "9.x",
    "prettier": "3.x"
  },
  "lint-staged": {
    "*.{js,json,css,md,yml}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
