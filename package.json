{
  "name": "@shelf/eslint-config",
  "version": "5.4.1",
  "description": "ESLint Config for Shelf Projects",
  "license": "MIT",
  "repository": "shelfio/eslint-config",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": "^20.19.0 || ^22.13.0 || >=24.0.0"
  },
  "scripts": {
    "test": "echo ok",
    "prepare": "husky",
    "lint": "eslint . --fix"
  },
  "files": [
    "common",
    "rules",
    "base.js",
    "frontend-typescript.js",
    "typescript.js"
  ],
  "main": "base.js",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "type": "module",
  "dependencies": {
    "@eslint/compat": "2.0.2",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "10.0.1",
    "@stylistic/eslint-plugin": "5.8.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.13.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx": "0.1.0",
    "eslint-plugin-n": "17.23.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-shelf-no-need-lodash-methods": "2.0.8",
    "eslint-plugin-sonarjs": "3.0.6",
    "eslint-plugin-testing-library": "7.15.4",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.14.0",
    "globals": "17.3.0",
    "typescript-eslint": "8.54.0"
  },
  "devDependencies": {
    "eslint": "10.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1"
  },
  "peerDependencies": {
    "eslint": "10.x",
    "prettier": "3.x"
  },
  "pnpm": {
    "overrides": {
      "eslint-plugin-jest>@typescript-eslint/utils": "8.54.0"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{html,md,yml}": "prettier --write",
    "*.{ts,js,json}": "eslint --fix"
  }
}
