{
  "name": "@shelf/eslint-config",
  "version": "0.0.6",
  "description": "ESLint Config for Shelf Projects",
  "license": "MIT",
  "repository": "shelfio/eslint-config",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8.10"
  },
  "files": [
    "rules",
    "backend.js",
    "base.js",
    "frontend.js",
    "meteor.js"
  ],
  "main": "base.js",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.1"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3"
  },
  "peerDependencies": {
    "eslint": "5.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
