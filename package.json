{
  "name": "@shelf/eslint-config",
  "version": "5.2.1",
  "description": "ESLint Config for Shelf Projects",
  "license": "MIT",
  "repository": "shelfio/eslint-config",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "test": "echo ok",
    "prepare": "husky",
    "lint": "eslint . --fix"
  },
  "files": [
    "common",
    "rules",
    "base.js",
    "frontend-typescript.js",
    "typescript.js"
  ],
  "main": "base.js",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "resolutions": {
    "strip-ansi": "7.1.0"
  },
  "type": "module",
  "dependencies": {
    "@eslint/compat": "1.3.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.29.0",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "@types/eslint__js": "9.14.0",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx": "0.1.0",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-shelf-no-need-lodash-methods": "2.0.8",
    "eslint-plugin-sonarjs": "3.0.3",
    "eslint-plugin-testing-library": "7.5.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.14.0",
    "globals": "16.2.0",
    "typescript-eslint": "8.35.0"
  },
  "devDependencies": {
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.6.0"
  },
  "peerDependencies": {
    "eslint": "9.x",
    "prettier": "3.x"
  },
  "lint-staged": {
    "*.{html,md,yml}": "prettier --write",
    "*.{ts,js,json}": "eslint --fix",
    ".circleci/config.yml": ".husky/validate-circleci-config.sh"
  },
  "publishConfig": {
    "access": "public"
  }
}
